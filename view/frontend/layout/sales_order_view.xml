<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
	<body>
		<referenceContainer name="order_info">
			<block class="Magento\Framework\View\Element\Template" name="click_collect_date" template="CravenDunnill_ClickCollect::order/view/collection_date.phtml" after="sales.order.info">
				<arguments>
					<argument name="order" xsi:type="object">Magento\Sales\Block\Order\Info\Interceptor</argument>
				</arguments>
			</block>
		</referenceContainer>
	</body>
</page>
